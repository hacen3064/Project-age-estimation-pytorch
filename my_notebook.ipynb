   "id": "ecb2a3f2",
   "metadata": {},
   "outputs": [],
   "source": [
    "data_dir = '../appa-real-release'"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "718e189b",
   "metadata": {},
   "outputs": [
    {
     "name": "stderr",
